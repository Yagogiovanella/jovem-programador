Program Pzim ;
Var
 a:real;
 b:real;
 soma,subtracao,multiplacacao,divisao:real;

 
Begin
writeln('digite numero a');
readln(a);
writeln('digite numero b');
readln(b);
 soma:=a+b;
 subtracao:=a-b;
 multiplicacao:=a*b;
 divisao:=a/b;
writeln(soma);
writeln(subtracao);
writeln(multiplicacao);
writeln(divisao);  
End.